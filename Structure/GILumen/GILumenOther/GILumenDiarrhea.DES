<structure><name> GILumenDiarrhea </name>

<variables>

<parm><name> H2OTarget </name><val> 0.0 </val></parm>

<var><name> H2OLoss </name></var>
<var><name> Na+Loss </name></var>
<var><name> HeatLoss </name></var>

</variables>

<functions>

<curve><name> H2OMassEffect </name>
  <point><x>  0.0 </x><y> 0.0 </y><slope> 0.0 </slope></point>
  <point><x> 50.0 </x><y> 1.0 </y><slope> 0.0 </slope></point>
</curve>

</functions>

<definitions>

<block><name> Dervs </name>

<def><name> H2OLoss </name><val> H2OTarget * H2OMassEffect [ GILumenVolume.Mass ] </val></def>
<def><name> Na+Loss </name><val> H2OLoss * GILumenSodium.[Na+] </val></def>
<def><name> HeatLoss </name><val> 0.001 * H2OLoss * GILumenTemperature.DegK </val></def>

</block>

</definitions>
</structure>

End