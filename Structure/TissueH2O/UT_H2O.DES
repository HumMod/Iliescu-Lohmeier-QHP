<structure><name> UT_H2O </name>

<variables>

<var><name> Gain </name></var>
<var><name> Loss </name></var>

<var><name> Vol(L) </name></var>

</variables>

<equations>

<diffeq>
  <name> Vol </name>
  <integralname> Vol </integralname>
  <initialval> 9800.0 </initialval>
  <dervname> Change </dervname>
  <errorlim> 98.0 </errorlim>
</diffeq>

</equations>

<definitions>
<block><name> Calc </name>

<def><name> Vol(L) </name><val> Vol / 1000.0 </val></def>

</block>

<block><name> Dervs </name>

<def><name> Gain </name>
  <val>
     UT_CapillaryWater.Rate
   + ( 0.250 * MetabolicH2O.Rate )
  </val>
</def>

<def><name> Loss </name>
  <val>
     UT_LymphWater.Rate
   + ( 0.333 * HeatSweating.H2O )
   + ( 0.333 * HeatInsensibleSkin.H2O )
  </val>
</def>


<def><name> Change </name><val> Gain - Loss </val></def>

</block>

</definitions>
</structure>

