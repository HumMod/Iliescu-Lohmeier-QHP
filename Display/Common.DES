  <repeat><reps> 500 </reps><stepsize> 0.1 </stepsize></repeat>
</repeatlist>

<repeatlist>
  <name> Common.Slope </name>
  <repeat><reps> 100 </reps><stepsize> 0.1 </stepsize></repeat>
</repeatlist>

<repeatlist>
  <name> Common.Clamp </name>
  <repeat><reps> 100 </reps><stepsize> 0.5 </stepsize></repeat>
</repeatlist>

